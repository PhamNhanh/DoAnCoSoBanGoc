@model WEBTimViec.Models.ChuyenNganh

<!DOCTYPE html>
<html lang="vi">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
</head>
<body>
    <p>
        <a href="@Url.Action("ListChuyenNganh")" class="btn btn-primary">Back to list</a>
    </p>
    <header>
        <h1>Thêm chuyên ngành</h1>
    </header>
    <div class="container">
        <form asp-action="AddChuyenNganh">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ChuyenNganh_name" class="control-label">Tên chuyên ngành</label>
                <input asp-for="ChuyenNganh_name" class="form-control" />
                <span asp-validation-for="ChuyenNganh_name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" />
            </div>
        </form>
    </div>

@*     <p>
        <a href="@Url.Action("IndexChuyenNganh")" class="btn btn-primary">Back to list</a>
    </p> *@
</body>
</html>
