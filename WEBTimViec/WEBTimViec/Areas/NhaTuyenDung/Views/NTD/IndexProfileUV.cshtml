    <link rel="stylesheet" href="~/css/slick.css">
    <link rel="stylesheet" href="~/css/nice-select.css">
    <link rel="stylesheet" href="~/css/style.css">
</head>
<body>
    <div class="container">
        <h1>Thông tin ứng viên</h1>
        <p>Tên ứng viên: @Model.FullName</p>
        <p>Email: @Model.Email</p>
        <p>Số điện thoại: @Model.SDT_UngVien</p>
        <p>Địa chỉ: @Model.DiaChi</p>
        <p>Ngày sinh: @Model.NgaySinh</p>
        <p>Giới thiệu bản thân: @Model.TuGioiThieu</p>

        <!-- Thêm các trường thông tin khác tùy theo nhu cầu -->
